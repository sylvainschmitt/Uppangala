	file = {10.1038@nature05134.pdf:/home/sylvain/.mozilla/firefox/a6j45cit.default/zotero/storage/SI9S3HN9/10.1038@nature05134.pdf:application/pdf;Snapshot:/home/sylvain/.mozilla/firefox/a6j45cit.default/zotero/storage/N4RGB6CP/nature05134.html:text/html}
