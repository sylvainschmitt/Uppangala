	month = jan,
	year = {2014},
	keywords = {Alps, fourth-corner matrix, functional ecology, permutation procedures, RLQ tables, trait–environment relationship},
	pages = {14--21},
	file = {Full Text PDF:/home/sylvain/.mozilla/firefox/a6j45cit.default/zotero/storage/XPZKZNWG/Dray et al. - 2014 - Combining the fourth-corner and the RLQ methods fo.pdf:application/pdf;Snapshot:/home/sylvain/.mozilla/firefox/a6j45cit.default/zotero/storage/AUM6KAJ7/abstract.html:text/html}
